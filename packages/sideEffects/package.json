{
  "name": "@samagra-x/uci-side-effects",
  "version": "0.0.6",
  "description": "Library of independent side effects that represent secondary functionalities of a transformer.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "npm run build -- --watch",
    "test": "jest --no-cache",
    "test:silent": "jest --silent"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "author": "chinmoy12c",
  "license": "MIT",
  "dependencies": {
    "@samagra-x/xmessage": "^1.0.16",
    "axios": "^1.6.5",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "tsup": "^8.0.0",
    "typescript": "^4.3.5",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/uuid": "^9.0.8",
    "fetch-mock": "^11.1.3",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^3.3.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.7.1"
  }
}
