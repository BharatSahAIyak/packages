{
  "name": "@samagra-x/uci-transformers",
  "version": "1.4.1",
  "description": "Library of services, actions and gaurds used to create any custom bot using Xstate configuration.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "npm run build -- --watch",
    "test": "jest --no-cache",
    "test:silent": "jest --silent"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "author": "Amruth-Vamshi",
  "license": "MIT",
  "dependencies": {
    "@samagra-x/uci-side-effects": "^0.0.5",
    "@samagra-x/xmessage": "^1.1.7",
    "axios": "^1.6.5",
    "form-data": "^4.0.0",
    "isolated-vm": "^4.7.2",
    "jest": "^29.7.0",
    "llamaindex": "0.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "openai": "^4.21.0",
    "qs": "^6.12.0",
    "sanitize-html": "^2.12.0",
    "tsup": "^8.0.0",
    "typescript": "^4.3.5",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/uuid": "^9.0.8",
    "fetch-mock": "^9.11.0",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^2.6.13",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1"
  }
}
